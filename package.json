{
  "name": "student-notebook-tracer",
  "displayName": "Student Notebook Tracer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": ["ms-toolsai.jupyter"],
  "activationEvents": [
    "onWebviewPanel:*.jupyter.notebook.ipynb"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "student-notebook-tracer.helloWorld",
      "title": "Hello World"
    },
  {
    "command": "student-notebook-tracer.execCount",
    "title": "Trace Execution Count" 
  }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
